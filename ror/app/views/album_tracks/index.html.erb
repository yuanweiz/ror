<p id="notice"><%= notice %></p>

<h1>Album Tracks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @album_tracks.each do |album_track| %>
      <tr>
        <td><%= link_to 'Show', album_track %></td>
        <td><%= link_to 'Edit', edit_album_track_path(album_track) %></td>
        <td><%= link_to 'Destroy', album_track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Album Track', new_album_track_path %>
