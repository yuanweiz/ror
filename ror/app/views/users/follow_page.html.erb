<div  id = "rwarp" class="col-lg-11  ">
	<% provide(:title, @title) %>
	<div class="row">
		<div class="col-md-11 ">
			<h3><%= @title %></h3>

				<table>
					<tr>
							<th>User Name</th>
							<th>User Location</th>
							<th>Follow/Unfollow</th>
					</tr>
					<% @users.each do |u| %>
					<tr>
							<td><%=link_to u.uname, playlists_user_path(u)%></td>
							<td><%=u.ucity%></td>
							<td><%= render partial: 'users/follow_or_unfollow_href',locals:{uid:u.uid} %></td>
					</tr>
					<%end%>
				</table>

		</div>
	</div>
</div>
