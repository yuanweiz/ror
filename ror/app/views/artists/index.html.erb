<div id = "rwarp" class="col-lg-11  ">
	<p id="notice"><%= notice %></p>

	<h1>Artists</h1>

	<table>
		<tr>
		  <th>Artist Name</th>
		  <th colspan="3">Profile</th>
		</tr>

		<% @artists.each do |artist| %>
		  <tr>
			<td><%= artist.aname %></td>
			<td><%= link_to 'Show', artist %></td>
		  </tr>
		<% end %>
	</table>

	<br>

</div>